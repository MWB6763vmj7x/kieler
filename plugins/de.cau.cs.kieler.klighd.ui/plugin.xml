<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.cau.cs.kieler.klighd"
            name="KIELER Light-weight Diagrams">
      </category>
      <wizard
            category="de.cau.cs.kieler.klighd"
            class="de.cau.cs.kieler.klighd.ui.wizard.KlighdExecutableExtensionFactory:de.cau.cs.kieler.klighd.ui.wizard.KlighdNewProjectWizard"
            hasPages="true"
            icon="icons/new_kproj.gif"
            id="de.cau.cs.kieler.klighd.ui.newKlighdProjectWizard"
            name="KLighD Project"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonWizard
            type="new"
            wizardId="de.cau.cs.kieler.klighd.ui.newKlighdProjectWizard">
         <enablement>
            <not>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
               </or>
            </not>
         </enablement>
      </commonWizard>
   </extension>

</plugin>
